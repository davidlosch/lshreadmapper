TEMPLATE = app
CONFIG += console
CONFIG += c++11
CONFIG -= app_bundle
CONFIG -= qt
QMAKE_CXXFLAGS += -std=c++11
#QMAKE_CXXFLAGS += -std=c++11 -O3 -march=native

# Enable openmp (may not work with Clang and MSVC)
*-g++* {
    QMAKE_CXXFLAGS += -fopenmp
    QMAKE_CXXFLAGS += -Wno-deprecated-declarations -Wno-deprecated-register
    QMAKE_LFLAGS   += -fopenmp
}

clang* {
    QMAKE_CXXFLAGS += -Wno-deprecated-declarations -Wno-deprecated-register
}

# Enable gzip support in seqan. Disable if you're missing zlib.
DEFINES += SEQAN_HAS_ZLIB
LIBS += -lz

INCLUDEPATH += vendor/seqan/include

